<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel >
        <bindings>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IServiceEngine">
                    <security>
                        <message clientCredentialType="UserName" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
      <behaviors>
        <endpointBehaviors>
          <behavior name="certificateEndpointBehavior">
            <clientCredentials>
              <serviceCertificate>
                <authentication certificateValidationMode="PeerOrChainTrust" />
              </serviceCertificate>
            </clientCredentials>
          </behavior>
        </endpointBehaviors>
      </behaviors>
        <client>
            <endpoint address="http://localhost:9109/ServiceEngineSvc.svc"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IServiceEngine"
                contract="ServiceClient.IServiceEngine" name="WSHttpBinding_IServiceEngine">
                <identity>
                    <certificate encodedValue="" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>